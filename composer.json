{
    "name": "leoleva/slim-rate-middleware",
    "description": "Rate middleware for slim php applications",
    "authors": [
        {
            "name": "Edvinas Pocius",
            "email": "ptmedvinas@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "psr/cache": "^1.0",
        "psr/http-message": "^1.0",
        "slim/slim": "3.*",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Nico\\SlimRateMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nico\\SlimRateMiddleware\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
